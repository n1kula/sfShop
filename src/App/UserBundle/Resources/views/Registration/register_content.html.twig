{% trans_default_domain 'FOSUserBundle' %}

<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="form-signin">
    {% block field_row %}
    
        
        {{ form_errors(form) }}
        {{ form_widget(form) }}
    
{% endblock field_row %}

{% block form_widget %}
    <ul {{ block('container_attributes') }}>
        {{ block('field_rows') }}
        {{ form_rest(form) }}
    </ul>
{% endblock form_widget %}
    <div>
        <input type="submit" value="{{ 'registration.submit'|trans }}" class="btn btn-lg btn-primary btn-block" />
    </div>
</form>
